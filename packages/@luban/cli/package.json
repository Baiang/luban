{
  "name": "@luban-cli/cli",
  "version": "0.0.2",
  "description": "a scaffold to create a react app",
  "bin": {
    "luban": "bin/index.js"
  },
  "keywords": [
    "cli",
    "react",
    "scaffold"
  ],
  "scripts": {
    "build:types": "tsc --pretty --project tsconfig.type.json",
    "build": "rimraf dist && tsc --pretty --project tsconfig.json && yarn run build:types",
    "postinstall": "node postInstall.js",
    "check:type": "tsc --noEmit",
    "watch": "yarn run build -- --watch --diagnostics"
  },
  "author": "front-end-captain <18810940932@163.com>",
  "homepage": "https://github.com/front-end-captain/luban/blob/master/packages/%40luban/cli/README.md",
  "license": "MIT",
  "directories": {
    "src": "src",
    "test": "__tests__"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/front-end-captain/luban.git"
  },
  "bugs": {
    "url": "https://github.com/front-end-captain/luban/issues"
  },
  "dependencies": {
    "@luban-cli/cli-shared-utils": "0.0.2",
    "chalk": "^3.0.0",
    "commander": "^4.1.0",
    "deepmerge": "^4.2.2",
    "didyoumean": "^1.2.1",
    "ejs": "^3.0.1",
    "envinfo": "^7.5.0",
    "inquirer": "^7.0.4",
    "isbinaryfile": "^4.0.4",
    "javascript-stringify": "^2.0.1",
    "js-yaml": "^3.13.1",
    "lodash.clonedeep": "^4.5.0",
    "minimist": "^1.2.0",
    "recast": "^0.18.5",
    "validate-npm-package-name": "^3.0.0",
    "yaml-front-matter": "^4.1.0",
    "semver": "^7.1.2"
  },
  "devDependencies": {
    "@luban-cli/cli-shared-types": "0.0.2",
    "@types/didyoumean": "^1.2.0",
    "@types/ejs": "^3.0.0",
    "@types/inquirer": "^6.5.0",
    "@types/js-yaml": "^3.12.2",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/minimist": "^1.2.0",
    "@types/resolve": "^1.14.0",
    "@types/validate-npm-package-name": "^3.0.0"
  },
  "engines": {
    "node": ">=10"
  }
}
